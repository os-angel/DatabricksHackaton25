# Databricks Configuration
DATABRICKS_HOST=https://your-workspace.cloud.databricks.com
DATABRICKS_TOKEN=your_access_token_here

# Databricks SQL Warehouse
DATABRICKS_WAREHOUSE_ID=your_warehouse_id

# Unity Catalog
CATALOG_NAME=biolabs_catalog

# Vector Search
VECTOR_SEARCH_ENDPOINT=biolabs_vector_endpoint

# MCP Server
MCP_SERVER_HOST=localhost
MCP_SERVER_PORT=8000

# LLM APIs (Optional - if not using Databricks Foundation Models)
ANTHROPIC_API_KEY=sk-ant-...
OPENAI_API_KEY=sk-...

# Gradio App
GRADIO_SERVER_PORT=7860
GRADIO_SHARE=false

# Logging
LOG_LEVEL=INFO
