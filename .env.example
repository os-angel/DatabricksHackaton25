# DecisionMakingArena Configuration
# Copy this file to .env and fill in your values

# Databricks Configuration
DATABRICKS_WORKSPACE_URL=https://your-workspace.cloud.databricks.com
DATABRICKS_TOKEN=your_databricks_token_here
DATABRICKS_WAREHOUSE_ID=your_warehouse_id  # Optional

# Genie Space IDs
# Create Genie Spaces in Databricks and add their IDs here
SALES_GENIE_SPACE_ID=your_sales_genie_space_id
FINANCE_GENIE_SPACE_ID=your_finance_genie_space_id
STRATEGIC_GENIE_SPACE_ID=your_strategic_genie_space_id

# Vector Search Configuration
VECTOR_SEARCH_ENDPOINT_NAME=decision_making_vs_endpoint
VECTOR_SEARCH_INDEX_NAME=decision_making_knowledge_base

# Model Configuration
# Use Databricks Foundation Models
MODEL_ORCHESTRATOR_MODEL=databricks-meta-llama-3-1-405b-instruct
MODEL_CLASSIFIER_MODEL=databricks-meta-llama-3-1-70b-instruct
MODEL_ANALYZER_MODEL=databricks-meta-llama-3-1-70b-instruct

# Application Settings
DEBUG=false
APP_MAX_CONVERSATION_HISTORY=10
APP_ENABLE_CACHING=true
APP_CACHE_TTL_SECONDS=300
